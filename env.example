# Contoh file environment variables
# Salin file ini ke .env dan sesuaikan dengan konfigurasi Anda

# URL website target untuk testing
TEST_URL=https://example.com/login

# Kredensial login
TEST_USERNAME=test@example.com
TEST_PASSWORD=password123

# Basic Authentication (untuk website yang memerlukan basic auth)
BASIC_AUTH_ENABLED=false
BASIC_AUTH_USERNAME=basicuser
BASIC_AUTH_PASSWORD=basicpass

# Session/Cookie options
USE_COOKIE=false
COOKIE_FILE=auth.json

# Browser options
HEADLESS=false
TIMEOUT=30000

# Logging level (error, warn, info, debug)
LOG_LEVEL=info

# Output file untuk hasil testing
OUTPUT_FILE=test-results.json 